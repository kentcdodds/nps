{
  "name": "p-s",
  "version": "0.0.0-semantically-released",
  "description": "All the benefits of npm scripts without the cost of a bloated package.json and limits of json",
  "main": "dist/index",
  "scripts": {
    "start": "p-s",
    "test": "p-s test",
    "localstart": "npm start build && ./dist/bin/p-s.js"
  },
  "bin": {
    "p-s": "./dist/bin/p-s.js",
    "package-scripts": "./dist/bin/p-s.js"
  },
  "files": [
    "bin",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "dependencies": {
    "arrify": "1.0.1",
    "async": "2.0.0-rc.6",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "find-up": "1.1.2",
    "lodash.camelcase": "4.1.1",
    "lodash.contains": "2.4.3",
    "lodash.find": "4.4.0",
    "lodash.isempty": "4.2.1",
    "lodash.isfunction": "3.0.8",
    "lodash.isplainobject": "4.0.4",
    "lodash.isstring": "4.0.1",
    "lodash.isundefined": "3.0.1",
    "lodash.kebabcase": "4.0.1",
    "lodash.merge": "4.4.0",
    "lodash.remove": "4.5.0",
    "prefix-matches": "0.0.9",
    "shell-escape": "0.2.0",
    "spawn-command": "0.0.2"
  },
  "devDependencies": {
    "all-contributors-cli": "3.0.6",
    "ava": "0.15.2",
    "babel-cli": "6.10.1",
    "babel-eslint": "6.1.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-2": "6.11.0",
    "babel-register": "6.9.0",
    "codecov": "1.0.1",
    "commitizen": "2.8.2",
    "condition-node-version": "1.3.0",
    "cz-conventional-changelog": "1.1.6",
    "eslint": "3.0.1",
    "eslint-config-kentcdodds": "8.0.1",
    "ghooks": "1.3.2",
    "nyc": "6.6.1",
    "opt-cli": "1.5.1",
    "p-s": "*",
    "proxyquire": "1.7.10",
    "rimraf": "2.5.3",
    "semantic-release": "4.3.5",
    "sinon": "1.17.4",
    "validate-commit-msg": "2.6.1"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/ava"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "exclude": [
      "**/*.test.js",
      "package-scripts.js"
    ]
  },
  "release": {
    "verifyConditions": {
      "path": "condition-node-version",
      "node": "^6",
      "verbose": true
    }
  },
  "config": {
    "ghooks": {
      "commit-msg": "opt --in commit-msg --exec \"validate-commit-msg\"",
      "pre-commit": "opt --in pre-commit --exec \"npm start validate\""
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/p-s.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/p-s/issues"
  },
  "homepage": "https://github.com/kentcdodds/p-s#readme"
}
