{
  "testPathDirs": [
    "<rootDir>/src"
  ],
  "testEnvironment": "jest-environment-node",
  "coveragePathIgnorePatterns": ["/node_modules/", "/fixtures/", "/helpers/"],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
